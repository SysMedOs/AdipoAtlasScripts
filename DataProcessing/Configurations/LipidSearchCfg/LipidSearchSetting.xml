        <database-group pcdb="general" pcid="Orbitrap" id="CID">
            <database-item database="GPL,GL,SL"/>
            <database-item database="SP,Cer,SPD"/>
            <database-item database="ChE"/>
            <database-item database="Ch"/>
            <database-item database="Co"/>
            <database-item database="CL"/>
            <database-item database="GGL"/>
            <database-item database="FAmid"/>
